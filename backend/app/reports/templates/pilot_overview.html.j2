<!DOCTYPE html>
<html>
<head><meta charset="utf-8"><title>{{ title }}</title></head>
<body>
<h1>{{ title }}</h1>
<section>
  <h2>KPIs</h2>
  <ul>
  {% for item in kpis %}
    <li>{{ item.name }}: {{ item.value }}</li>
  {% endfor %}
  </ul>
</section>
<section>
  <h2>Outcomes</h2>
  <table border="1">
    <tr><th>Name</th><th>Result</th></tr>
    {% for o in outcomes %}
    <tr><td>{{ o.name }}</td><td>{{ o.result }}</td></tr>
    {% endfor %}
  </table>
</section>
</body>
</html>
